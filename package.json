{
  "name": "geo2",
  "version": "1.8.7",
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:wsl": "ng serve --live-reload=false --configuration=dev",
    "start:dev": "ng serve --open --live-reload=true",
    "start:webdev": "ng serve --open --live-reload=false --configuration=webdev",
    "start:bw": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production && node script/archive ./dist/geo2-{version}.tar.gz ./dist/geo2/",
    "build:webdev": "ng build --configuration=webdev",
    "build:bwdev": "ng build --configuration=bwdev && node script/archive ./dist/geo2-bwtest-{version}.tar.gz ./dist/geo2/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/geo2/stats.json",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=HeadlessChrome",
    "lint:check": "npx tsc && npx prettier ./src/**/*.{scss,html} && ng lint",
    "lint:fix": "npx tsc && npx prettier --write ./src/**/*.{scss,html} && ng lint --fix",
    "e2e": "ng e2e",
    "e2e:no-serve": "ng e2e --devServerTarget=",
    "e2e:headless": "ng e2e --configuration=headless",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes",
    "devx": "devextreme",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@apollo/client": "^3.7.12",
    "apollo-angular": "^4.2.1",
    "devextreme": "22.2.5",
    "devextreme-angular": "22.2.5",
    "devextreme-schematics": "^1.2.7",
    "graphql": "^16.6.0",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cdk": "^14.2.7",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.6.61",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "archiver": "^5.1.0",
    "devextreme-cli": "^1.2.7",
    "devextreme-themebuilder": "22.2.5",
    "eslint": "^8.23.0",
    "husky": "^7.0.4",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "4.8",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
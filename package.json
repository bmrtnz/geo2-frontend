{
  "name": "geo2",
  "version": "1.8.5",
  "engines": {
    "npm": ">=6",
    "node": ">=14"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:wsl": "ng serve --liveReload=false --optimization=false --source-map=true",
    "start:dev": "ng serve --open --liveReload=true",
    "start:webdev": "ng serve --open --liveReload=false --configuration=webdev",
    "start:bw": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod && node script/archive ./dist/geo2-{version}.tar.gz ./dist/geo2/",
    "build:webdev": "ng build --configuration=webdev",
    "build:bwdev": "ng build --configuration=bwdev && node script/archive ./dist/geo2-bwtest-{version}.tar.gz ./dist/geo2/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/geo2/stats.json",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=HeadlessChrome",
    "lint:check": "npx tsc && npx prettier ./src/**/*.{scss,html} && ng lint",
    "lint:fix": "npx tsc && npx prettier --write ./src/**/*.{scss,html} && ng lint --fix",
    "e2e": "ng e2e",
    "e2e:no-serve": "ng e2e --devServerTarget=",
    "e2e:headless": "ng e2e --configuration=headless",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes && ngcc",
    "devx": "devextreme",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@apollo/client": "^3.3.0",
    "apollo-angular": "^2.1.0",
    "apollo-angular-link-http": "^1.11.0",
    "devextreme": "^20.2.11",
    "devextreme-angular": "^20.2.11",
    "devextreme-schematics": "^1.2.7",
    "graphql": "^15.0.0",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^1.14.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.4",
    "@angular/cli": "^10.2.4",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "~10.2.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.6.61",
    "archiver": "^5.1.0",
    "codelyzer": "^6.0.1",
    "devextreme-cli": "^1.2.7",
    "devextreme-themebuilder": "^20.2.11",
    "husky": "^7.0.4",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.6.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}

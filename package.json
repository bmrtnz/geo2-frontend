{
  "name": "geo2",
  "version": "1.12.0",
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "private": true,
  "scripts": {
    "postinstall": "npm run build-themes",
    "start": "ng serve --open",
    "start:wsl": "ng serve --live-reload=false --configuration=dev",
    "start:dev": "ng serve --open --live-reload=true",
    "start:webdev": "ng serve --open --live-reload=false --configuration=webdev",
    "start:bw": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production && node script/archive ./dist/geo2-{version}.tar.gz ./dist/geo2/",
    "build:webdev": "ng build --configuration=webdev",
    "build:bwdev": "ng build --configuration=bwdev && node script/archive ./dist/geo2-bwtest-{version}.tar.gz ./dist/geo2/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/geo2/stats.json",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=HeadlessChrome",
    "lint": "ng lint",
    "lint:check": "npx prettier -c ./src && npm run lint",
    "lint:fix": "npx prettier -c -w ./src && npm run lint -- --fix",
    "e2e": "ng e2e",
    "e2e:no-serve": "ng e2e --devServerTarget=",
    "e2e:headless": "ng e2e --configuration=headless",
    "build-themes": "npx --yes -p devextreme-cli devextreme build",
    "ng": "ng",
    "devx": "devextreme"
  },
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@apollo/client": "^3.7.12",
    "apollo-angular": "^4.2.1",
    "devextreme": "22.2.5",
    "devextreme-angular": "22.2.5",
    "devextreme-schematics": "^1.5.1",
    "graphql": "^16.6.0",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^6.7.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.4",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cdk": "^15.2.8",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "archiver": "^5.1.0",
    "devextreme-cli": "^1.2.7",
    "devextreme-themebuilder": "22.2.5",
    "eslint": "^8.33.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.8.8",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}

{
  "name": "geo2",
  "version": "1.8.7",
  "engines": {
    "npm": ">=6",
    "node": ">=14"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:wsl": "ng serve --liveReload=false --configuration=dev",
    "start:dev": "ng serve --open --liveReload=true",
    "start:webdev": "ng serve --open --liveReload=false --configuration=webdev",
    "start:bw": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod && node script/archive ./dist/geo2-{version}.tar.gz ./dist/geo2/",
    "build:webdev": "ng build --configuration=webdev",
    "build:bwdev": "ng build --configuration=bwdev && node script/archive ./dist/geo2-bwtest-{version}.tar.gz ./dist/geo2/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/geo2/stats.json",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=HeadlessChrome",
    "lint:check": "npx tsc && npx prettier ./src/**/*.{scss,html} && ng lint",
    "lint:fix": "npx tsc && npx prettier --write ./src/**/*.{scss,html} && ng lint --fix",
    "e2e": "ng e2e",
    "e2e:no-serve": "ng e2e --devServerTarget=",
    "e2e:headless": "ng e2e --configuration=headless",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes && ngcc",
    "devx": "devextreme",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@apollo/client": "^3.3.0",
    "apollo-angular": "^2.1.0",
    "apollo-angular-link-http": "^1.11.0",
    "devextreme": "^20.2.11",
    "devextreme-angular": "^20.2.11",
    "devextreme-schematics": "^1.2.7",
    "graphql": "^15.0.0",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.19",
    "@angular-eslint/builder": "4.3.1",
    "@angular-eslint/eslint-plugin": "4.3.1",
    "@angular-eslint/eslint-plugin-template": "4.3.1",
    "@angular-eslint/schematics": "4.3.1",
    "@angular-eslint/template-parser": "4.3.1",
    "@angular/cli": "^11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.6.61",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "archiver": "^5.1.0",
    "devextreme-cli": "^1.2.7",
    "devextreme-themebuilder": "^20.2.11",
    "eslint": "^7.6.0",
    "husky": "^7.0.4",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
